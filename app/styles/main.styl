body
	font: 12px Helvetica, Arial, sans-serif

border-radius()
	-webkit-border-radius: arguments
	-moz-border-radius: arguments
	border-radius: arguments

a.button
	border-radius(5px)


header
	border-radius(10px)
	background #9459A1
	color #FFF
	font-size 1.2em
	padding 1em

.content
	border-radius(12px)
	border 2px solid #9459A1
	color #9459A1
	margin 1em 0 1em 0
	padding 1em

footer
	border-radius(10px)
	background #9459A1
	color #FFF
	font-size 1.2em
	padding 1em